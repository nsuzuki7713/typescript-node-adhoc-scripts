{
  "name": "typescript-node-adhoc-scripts",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node",
    "test": "jest --verbose",
    "lint": "eslint --ext .ts,.js src/",
    "lint:fix": "eslint --fix --ext .ts,.js src/"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.4",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^25.4.0",
    "prettier": "^2.0.5",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.9.1",
    "typescript": "^3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ],
    "testEnvironment": "node",
    "collectCoverage": false,
    "coverageReporters": [
      "text",
      "lcov"
    ]
  },
  "dependencies": {
    "@octokit/rest": "^18.0.4",
    "@types/lodash": "^4.14.170",
    "aws-sdk": "^2.781.0",
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "iconv-lite": "^0.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.27.0",
    "node-html-parser": "^1.2.20"
  }
}
